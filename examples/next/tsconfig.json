{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@cartridge/tsconfig/react.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "noEmit": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "tests"
  ]
}
