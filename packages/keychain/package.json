{
  "name": "@cartridge/keychain",
  "version": "0.12.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "env-cmd -f .env.dev vite",
    "dev:live": "env-cmd -f .env.live vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview --port 3001",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ci": "vitest run",
    "storybook": "storybook dev -p 6001 --no-open",
    "storybook:build": "storybook build",
    "storybook:serve": "pnpm storybook:build --quiet && pnpm http-server -c-1 storybook-static --port 6001 --silent",
    "test-storybook": "test-storybook --testTimeout=60000",
    "test:storybook:update": "start-server-and-test 'SNAPSHOT=1 pnpm storybook:serve' 6001 'pnpm test-storybook --url http://127.0.0.1:6001 -u'",
    "test:storybook": "start-server-and-test 'pnpm storybook:serve' 6001 'pnpm test-storybook -u'"
  },
  "imports": {
    "@/hooks/account": {
      "storybook": "./src/profile/hooks/account.mock.ts",
      "default": "./src/profile/hooks/account.ts"
    },
    "@/hooks/collection": {
      "storybook": "./src/profile/hooks/collection.mock.ts",
      "default": "./src/profile/hooks/collection.ts"
    },
    "@/hooks/token": {
      "storybook": "./src/profile/hooks/token.mock.ts",
      "default": "./src/profile/hooks/token.ts"
    },
    "@/*": [
      "./src/profile/*",
      "./src/profile/*.ts",
      "./src/profile/*.tsx",
      "./src/profile/*/index.ts",
      "./src/profile/*/index.tsx"
    ]
  },
