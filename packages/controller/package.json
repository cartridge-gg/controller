{
  "name": "@cartridge/controller",
  "version": "0.5.1",
  "description": "Cartridge Controller",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build:deps": "tsup --dts-resolve",
    "build": "pnpm build:deps",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "version": "pnpm pkg get version"
  },
  "exports": {
    ".": "./dist/index.js",
    "./session": "./dist/session/index.js",
    "./provider": "./dist/provider/index.js",
    "./types": "./dist/types/index.js"
  },
  "tsup": {
    "entry": [
      "src/**"
    ],
    "format": [
      "esm"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "dependencies": {
    "@cartridge/account-wasm": "workspace:*",
    "@cartridge/penpal": "^6.2.3",
    "@cartridge/presets": "workspace:*",
    "@starknet-io/types-js": "^0.7.7",
    "@telegram-apps/sdk": "^2.4.0",
    "base64url": "^3.0.1",
    "cbor-x": "^1.5.0",
    "fast-deep-equal": "^3.1.3",
    "query-string": "^7.1.1",
    "starknet": "^6.11.0"
  },
  "devDependencies": {
    "@cartridge/tsconfig": "workspace:*",
    "@types/node": "^20.6.0",
    "typescript": "^5.4.5"
  }
}