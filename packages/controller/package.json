{
  "name": "@cartridge/controller",
  "version": "0.8.0",
  "description": "Cartridge Controller",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build:deps": "bun run build",
    "dev": "vite build --watch",
    "build:browser": "vite build",
    "build:node": "tsup --config tsup.node.config.ts",
    "build": "bun run build:browser && bun run build:node",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "version": "bun pm ls --depth=0 | grep $(pwd) | cut -d@ -f2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./session": {
      "types": "./dist/session.d.ts",
      "import": "./dist/session.js"
    },
    "./session/node": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/node/index.js",
      "require": "./dist/node/index.cjs"
    }
  },
  "peerDependencies": {
    "@metamask/sdk": "^0.32.1",
    "@solana/web3.js": "^1.98.0",
    "open": "^10.1.0",
    "starknet": "^6.21.0",
    "starknetkit": "^2.6.1"
  },
  "dependencies": {
    "@cartridge/controller-wasm": "0.1.8",
    "@cartridge/penpal": "^6.2.4",
    "ethers": "^6.13.5",
    "@starknet-io/types-js": "^0.7.7",
    "@telegram-apps/sdk": "^2.4.0",
    "@turnkey/sdk-browser": "^4.0.0",
    "cbor-x": "^1.5.0",
    "mipd": "^0.0.7",
    "@walletconnect/ethereum-provider": "^2.20.0"
  },
  "devDependencies": {
    "@cartridge/tsconfig": "workspace:*",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.0.6",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.0.1",
    "typescript": "^5.7.3",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vite-plugin-top-level-await": "^1.4.4",
    "vite-plugin-wasm": "^3.4.1"
  }
}
