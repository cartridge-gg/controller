{
  "name": "@cartridge/profile",
  "version": "0.8.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "format": "prettier --write ./src index.html",
    "format:check": "prettier --check ./src index.html",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6003 --no-open",
    "storybook:build": "storybook build",
    "storybook:serve": "bun run storybook:build --quiet && bun run http-server -c-1 storybook-static --port 6003 --silent",
    "test-storybook": "test-storybook --testTimeout=60000",
    "test:storybook:update": "start-server-and-test 'SNAPSHOT=1 bun run storybook:serve' 6003 'bun run test-storybook --url http://127.0.0.1:6003 -u'",
    "test:storybook": "start-server-and-test 'bun run storybook:serve' 6003 'bun run test-storybook --url http://127.0.0.1:6003'"
  },
  "imports": {
    "#hooks/account": {
      "storybook": "./src/hooks/account.mock.ts",
      "default": "./src/hooks/account.ts"
    },
    "#hooks/collection": {
      "storybook": "./src/hooks/collection.mock.ts",
      "default": "./src/hooks/collection.ts"
    },
    "#hooks/token": {
      "storybook": "./src/hooks/token.mock.ts",
      "default": "./src/hooks/token.ts"
    },
    "#*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts",
      "./src/*/index.tsx"
    ]
  },
  "dependencies": {
    "@cartridge/arcade": "0.0.0",
    "@cartridge/controller": "workspace:*",
    "@cartridge/marketplace": "0.0.9",
    "@cartridge/penpal": "^6.2.4",
    "@cartridge/ui": "github:cartridge-gg/ui#bfcc1bd",
    "@dojoengine/torii-wasm": "1.5.16",
    "compare-versions": "^6.1.1",
    "lodash": "^4.17.21",
    "qrcode.react": "^4.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.27.0",
    "sonner": "^1.4.41",
    "starknet": "^6.21.0",
    "viem": "^2.21.32",
    "vite-plugin-top-level-await": "^1.4.4",
    "vite-plugin-wasm": "^3.4.1"
  },
  "devDependencies": {
    "@cartridge/eslint": "workspace:*",
    "@cartridge/tsconfig": "workspace:*",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-themes": "^8.5.0",
    "@storybook/blocks": "^8.5.0",
    "@storybook/react": "^8.5.0",
    "@storybook/react-vite": "^8.5.0",
    "@storybook/test": "^8.5.0",
    "@storybook/test-runner": "^0.21.0",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/lodash": "^4.17.14",
    "@types/node": "^18.0.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.12.0",
    "globals": "^15.0.0",
    "http-server": "^14.1.1",
    "jest-image-snapshot": "^6.4.0",
    "postcss": "^8.4.35",
    "postcss-import": "^16.1.0",
    "prettier": "^3.4.2",
    "start-server-and-test": "^2.0.9",
    "storybook": "^8.5.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.0"
  }
}
