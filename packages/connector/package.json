{
  "name": "@cartridge/connector",
  "version": "0.8.0",
  "description": "Cartridge Controler Connector",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build:deps": "tsup --dts-resolve",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "exports": {
    ".": "./dist/index.js",
    "./controller": "./dist/controller.js",
    "./session": "./dist/session.js",
    "./telegram": "./dist/telegram.js"
  },
  "typesVersions": {
    "*": {
      "controller": [
        "./dist/controller.d.ts"
      ],
      "session": [
        "./dist/session.d.ts"
      ],
      "telegram": [
        "./dist/telegram.d.ts"
      ]
    }
  },
  "tsup": {
    "entry": [
      "src/controller.ts",
      "src/session.ts",
      "src/index.ts"
    ],
    "format": [
      "esm"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": "resolve"
  },
  "peerDependencies": {
    "@starknet-react/core": "^3.7.3"
  },
  "dependencies": {
    "@cartridge/controller": "workspace:*",
    "minimatch": "^10.0.3"
  },
  "devDependencies": {
    "@cartridge/tsconfig": "workspace:*",
    "prettier": "^3.4.2",
    "tsup": "^8.0.1",
    "typescript": "^5.7.3"
  }
}
