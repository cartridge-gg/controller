{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build:deps": {
      "dependsOn": ["^build:deps"],
      "outputs": ["dist/**"]
    },
    "build": {
      "dependsOn": ["^build:deps", "^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "persistent": true,
      "dependsOn": ["^build:deps", "^dev"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev:ci": {
      "persistent": true,
      "dependsOn": ["^build:deps", "^dev:ci"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "format": {},
    "e2e": {
      "persistent": true
    },
    "e2e:ui": {
      "persistent": true
    }
  }
}
